<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>Sss_Header</servlet-name>
        <servlet-class>by.belstu.istomin.servlets.Sss_Header</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_Header</servlet-name>
        <url-pattern>/SssHeader</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sss_XML</servlet-name>
        <servlet-class>by.belstu.istomin.servlets.Sss_XML</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_XML</servlet-name>
        <url-pattern>/SssXml</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Sss_JSON</servlet-name>
        <servlet-class>by.belstu.istomin.servlets.Sss_JSON</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Sss_JSON</servlet-name>
        <url-pattern>/SssJson</url-pattern>
    </servlet-mapping>
</web-app>